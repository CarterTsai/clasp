
include	$(CANDO_HOME)/makefile.inc

printenv:
	echo CANDO_APP_RESOURCES_LISP_DIR=$(CANDO_APP_RESOURCES_LISP_DIR)


subAll sa sad:
	make subBundle


subBundle sb:
	install -d $(CANDO_APP_RESOURCES_LISP_DIR)/CANDO/init/cmp
	install -C *.lsp $(CANDO_APP_RESOURCES_LISP_DIR)/CANDO/init/cmp


subInstall:
	@echo Install buildDatabase cando-script code, for now do nothing


subClean:
	@echo DO nothing

