

#
# Variables
#
#


$bond 1.4		# a typical bond
$resinBond 2.0
$longBond 1.9
$hbond 1.02
$angSp3 108
$angSp2 120
$interSp2 130
$dkpSp2 120
$dkpBond 1.4

$pg 0.8
$fbond 0.8

$rot2 -30
$rot3 210
$rot4 -30

$cy0 0
$cy1 12
$cy2 24
$cy3 36
$cy4 48
$cy5 60


#
# parts
#
! ester -10 0 0
1:H
1:E1:Cl - $bond 1:H

! boc -10 3 0
1:H
1:B1:I - $bond 1:H

! fmoc -10 6 0
1:H
1:F1:F - $bond 1:H

#
# Pro4 Building block 
#
! chain1 -10 10 0 

2:OAC:O
2:CAC -	$bond 2:OAC
2:OA =	$bond 2:CAC	-120 2:OAC
2:HO -	$hbond 2:OAC	120 2:CAC		0 2:OA
2:CA - 	$bond 2:CAC	$angSp2 2:OAC		180 2:OA
2:CB - 	$bond 2:CA	126 2:CAC		180 2:OAC
2:CG - 	$bond 2:CB	$angSp3 2:CA		-160 2:CAC
2:CD - 	$bond 2:CG	$angSp3 2:CB		0 2:CA
2:NA - 	$bond 2:CD	$angSp3 2:CG		0 2:CB
bond 2:NA - 2:CA
2:B3:I - $pg 2:NA	$angSp2 2:CA		180 2:CB

@center 2:NA 2:CA   # a dummy atom
	#
	# Build off of the quaternary center on CG
	#
2:NG -	$bond 2:CG	$angSp2 @center		$rot2 2:CA
2:HG -	$hbond 2:NG	$angSp2 2:CG		0 @center
2:CGC -	$bond 2:CG	$angSp2 2:NG		180 2:HG
2:OG =	$bond 2:CGC	$angSp2 2:CG		0 @center
2:E2:Cl - $pg 2:CGC	$angSp2 2:CG		0 2:NG

2:F2:F - $fbond 2:NG		$angSp2 2:CG		180 @center

#
# hin monomer
#
! hin -10 20 0
2:OAC:O
3:C2C -	$bond 2:OAC
