3:C8 -	$bond 3:C7	$angSp2 3:C6		0 3:C5
bond 3:C8 - 3:C9
	#
	# Now the quaternary center on C7
	#
3:N7 -	$bond 3:C7	$angSp2 3:C4		$rot3 3:C9
3:H7 -	$hbond 3:N7	$angSp2 3:C7		0 3:C4
3:C7C - $bond 3:C7	$angSp2 3:N7		180 3:H7
